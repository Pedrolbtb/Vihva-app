<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Inicio.Alarme"
    android:background="@drawable/background_home">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/close"
            android:layout_width="65dp"
            android:layout_height="59dp"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:contentDescription="@string/close_button_description"
            android:padding="15dp"
            android:src="@drawable/arrow_voltar_vermelho"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img_save_perfil"
            style="@style/foto"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:scaleType="centerCrop"
            app:civ_border_color="@color/Letras"
            app:civ_border_width="2dp"
            android:layout_marginBottom="480dp"
            android:layout_marginEnd="200dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/text_nome"
            android:layout_width="160dp"
            android:layout_height="30dp"
            android:layout_marginLeft="160dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:textSize="24sp"
            android:textColor="@color/Letras"
            android:layout_marginBottom="550dp"
            android:textAlignment="center"
            android:maxLines="1"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/crm"
            style="@style/text"
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="160dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:textSize="18sp"
            android:layout_marginBottom="465dp"
            android:textAlignment="center"
            android:maxLines="1"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_genero"
            style="@style/text"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="400dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:textAlignment="center"
            android:textSize="18sp"
            android:layout_marginLeft="160dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/exclui_medico"
            style="@style/Button_salvar"
            android:layout_width="160dp"
            android:layout_height="40dp"
            android:layout_marginEnd="190dp"
            android:width="180dp"
            android:background="@drawable/button_logout"
            android:elevation="2dp"
            android:text="@string/text_desfaz_amizade"
            android:textColor="@color/off_white"
            android:textSize="13sp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:layout_marginBottom="260dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/cardView2"
            style="@style/Edit_Text_Perfil"
            android:layout_width="357dp"
            android:layout_height="357dp"
            android:layout_marginTop="180dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView5"
            style="@style/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="18sp"
            android:layout_marginBottom="280dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <TextView
            android:id="@+id/centro_medico"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:layout_marginLeft="150dp"
            android:layout_marginBottom="280dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:maxLines="2"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <TextView
            android:id="@+id/textView6"
            style="@style/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="18sp"
            android:layout_marginBottom="200dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <TextView
            android:id="@+id/detalhes_clinica"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:layout_marginLeft="60dp"
            android:layout_marginBottom="200dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:maxLines="2"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <TextView
            android:id="@+id/textView4"
            style="@style/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Imagens da clÃ­nica "
            android:textSize="20sp"
            android:layout_marginBottom="40dp"
            android:fontFamily="@font/cloudsoft_bold_700"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2"
            app:layout_constraintVertical_chainStyle="spread" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/foto1"
            style="@style/foto"
            android:layout_width="94dp"
            android:layout_height="97dp"
            android:scaleType="centerCrop"
            android:layout_marginEnd="230dp"
            android:layout_marginTop="110dp"
            app:civ_border_color="@color/Letras"
            app:civ_border_width="1dp"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/foto2"
            style="@style/foto"
            android:layout_width="94dp"
            android:layout_height="97dp"
            android:scaleType="centerCrop"
            android:layout_marginTop="110dp"
            android:layout_marginEnd="1dp"
            app:civ_border_color="@color/Letras"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/foto3"
            style="@style/foto"
            android:layout_width="94dp"
            android:layout_height="97dp"
            android:layout_marginLeft="225dp"
            android:scaleType="centerCrop"
            android:layout_marginTop="110dp"
            app:civ_border_color="@color/Letras"
            app:civ_border_width="1dp"
            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
            app:layout_constraintEnd_toEndOf="@+id/cardView2"
            app:layout_constraintStart_toStartOf="@+id/cardView2"
            app:layout_constraintTop_toTopOf="@+id/cardView2" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="1.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>